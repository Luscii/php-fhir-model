{
    "name": "luscii/php-fhir-model",
    "type": "library",
    "license": "Apache-2.0",
    "description": "FHIR Model, parser and serializer",
    "keywords": [
        "hl7",
        "fhir",
        "model",
        "parser",
        "serializer",
        "json",
        "xml"
    ],
    "require": {
        "php": "^7.1 | ^8.0"
    },
    "require-dev": {
        "php": "^7.1| ^8.0",
        "ext-json": "*",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "HL7\\FHIR\\": "src/HL7/FHIR"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FHIR\\Tests\\": "tests"
        }
    }
}
